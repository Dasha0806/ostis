tenebrae_parmeliopsis => nrel_main_idtf:
[пармелиопсис темный](*<-lang_ru;;*);
[tenebrae parmeliopsis](*<-lang_lat;;*);
[пармелiепсiс цемны](*<-lang_bel;;*);;
tenebrae_parmeliopsis => nrel_idtf:[пармелiепсiс цемны](*<-lang_bel;;*);;

plants -> tenebrae_parmeliopsis;;
concept_species -> tenebrae_parmeliopsis;;
subject_domain_of_aves -> rrel_not_maximum_studied_object_class: tenebrae_parmeliopsis;;
